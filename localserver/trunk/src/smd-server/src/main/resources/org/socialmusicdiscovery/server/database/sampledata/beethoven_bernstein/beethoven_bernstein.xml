<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog/1.9"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog/1.9 http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-1.9.xsd">
    <changeSet id="sample_thebodyguard" author="erland">
        <loadData tableName="smdidentity_references"
                  file="org/socialmusicdiscovery/server/database/sampledata/beethoven_bernstein/smdidentity_references.csv">
            <column header="id" type="STRING"/>
            <column header="type" type="STRING"/>
        </loadData>
        <loadData tableName="labels"
                  file="org/socialmusicdiscovery/server/database/sampledata/beethoven_bernstein/labels.csv">
            <column header="id" type="STRING"/>
            <column header="name" type="STRING"/>
        </loadData>
        <loadData tableName="releases"
                  file="org/socialmusicdiscovery/server/database/sampledata/beethoven_bernstein/releases.csv">
            <column header="id" type="STRING"/>
            <column header="name" type="STRING"/>
            <column header="label_id" type="STRING"/>
        </loadData>
        <loadData tableName="persons"
                  file="org/socialmusicdiscovery/server/database/sampledata/beethoven_bernstein/persons.csv">
            <column header="id" type="STRING"/>
            <column header="name" type="STRING"/>
        </loadData>
        <loadData tableName="artists"
                  file="org/socialmusicdiscovery/server/database/sampledata/beethoven_bernstein/artists.csv">
            <column header="id" type="STRING"/>
            <column header="name" type="STRING"/>
        </loadData>
        <loadData tableName="works"
                  file="org/socialmusicdiscovery/server/database/sampledata/beethoven_bernstein/works.csv">
            <column header="id" type="STRING"/>
            <column header="name" type="STRING"/>
        </loadData>
        <loadData tableName="recording_sessions"
                  file="org/socialmusicdiscovery/server/database/sampledata/beethoven_bernstein/recording_sessions.csv">
            <column header="id" type="STRING"/>
        </loadData>
        <loadData tableName="release_recording_sessions"
                  file="org/socialmusicdiscovery/server/database/sampledata/beethoven_bernstein/release_recording_sessions.csv">
            <column header="release_id" type="STRING"/>
            <column header="session_id" type="STRING"/>
        </loadData>
        <loadData tableName="recordings"
                  file="org/socialmusicdiscovery/server/database/sampledata/beethoven_bernstein/recordings.csv">
            <column header="id" type="STRING"/>
            <column header="session_id" type="STRING"/>
            <column header="work_id" type="STRING"/>
            <column header="name" type="STRING"/>
        </loadData>
        <loadData tableName="tracks"
                  file="org/socialmusicdiscovery/server/database/sampledata/beethoven_bernstein/tracks.csv">
            <column header="id" type="STRING"/>
            <column header="release_id" type="STRING"/>
            <column header="recording_id" type="STRING"/>
            <column header="number" type="NUMERIC"/>
        </loadData>
        <loadData tableName="contributors"
                  file="org/socialmusicdiscovery/server/database/sampledata/beethoven_bernstein/contributors.csv">
            <column header="id" type="STRING"/>
            <column header="artist_id" type="STRING"/>
            <column header="recording_id" type="STRING"/>
            <column header="work_id" type="STRING"/>
            <column header="release_id" type="STRING"/>
            <column header="session_id" type="STRING"/>
            <column header="type" type="STRING"/>
        </loadData>
    </changeSet>
</databaseChangeLog>
