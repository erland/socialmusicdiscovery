<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog/1.9"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog/1.9 http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-1.9.xsd">
    <changeSet id="sample_crazyfrog_axelf" author="erland">
        <loadData tableName="smdentity_references"
                  file="org/socialmusicdiscovery/server/database/sampledata/crazyfrog_axelf/smdentity_references.csv">
            <column header="id" type="STRING"/>
            <column header="type" type="STRING"/>
        </loadData>
        <loadData tableName="artists"
                  file="org/socialmusicdiscovery/server/database/sampledata/crazyfrog_axelf/artists.csv">
            <column header="id" type="STRING"/>
            <column header="name" type="STRING"/>
        </loadData>
        <loadData tableName="works"
                  file="org/socialmusicdiscovery/server/database/sampledata/crazyfrog_axelf/works.csv">
            <column header="id" type="STRING"/>
            <column header="name" type="STRING"/>
        </loadData>
        <loadData tableName="recordings"
                  file="org/socialmusicdiscovery/server/database/sampledata/crazyfrog_axelf/recordings.csv">
            <column header="id" type="STRING"/>
            <column header="work_id" type="STRING"/>
            <column header="name" type="STRING"/>
        </loadData>
        <loadData tableName="tracks"
                  file="org/socialmusicdiscovery/server/database/sampledata/crazyfrog_axelf/tracks.csv">
            <column header="id" type="STRING"/>
            <column header="release_id" type="STRING"/>
            <column header="recording_id" type="STRING"/>
            <column header="number" type="NUMERIC"/>
        </loadData>
        <loadData tableName="contributors"
                  file="org/socialmusicdiscovery/server/database/sampledata/crazyfrog_axelf/contributors.csv">
            <column header="id" type="STRING"/>
            <column header="artist_id" type="STRING"/>
            <column header="recording_id" type="STRING"/>
            <column header="work_id" type="STRING"/>
            <column header="release_id" type="STRING"/>
            <column header="type" type="STRING"/>
        </loadData>
    </changeSet>
</databaseChangeLog>
